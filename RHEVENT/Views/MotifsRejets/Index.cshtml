@model IEnumerable<RHEVENT.Models.MotifsRejet>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}

<h2>Motifs de Rejet</h2>

<p>
    @Html.ActionLink("Nouveau Motif", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MotifRejet)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Conséquense)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MotifRejet)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Conséquense)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
